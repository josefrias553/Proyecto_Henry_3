# Configuración de AWS (Requerido)
AWS_ACCESS_KEY_ID=tu_access_key
AWS_SECRET_ACCESS_KEY=tu_secret_key
# Región de AWS (Opcional, por defecto: us-east-1)
AWS_REGION=us-east-1

# ===== RAW LAYER =====
# Bucket de S3 capa Raw
RAW_BUCKET=your-raw-bucket-name
# Prefijo/carpeta dentro del bucket
RAW_PREFIX=marketplace_rentals

# ===== BRONZE LAYER =====
# Bucket de S3 capa Bronze
BRONZE_BUCKET=your-bronze-bucket-name
# Prefijo/carpeta
BRONZE_PREFIX=marketplace_rentals

# Modo de ejecución (verification | production)
EXECUTION_MODE=verification

# Configuración de Dominio (Opcional)
# Se usa para naming de archivos (por defecto: marketplace_rentals)
DOMAIN=marketplace_rentals

# Parámetros de Extracción (Opcional - pueden pasarse por CLI)
# Valores válidos: api, scraping, csv
EXTRACTION_SOURCE=api
# Entidad a extraer (ej. listings, posts, neighborhoods, productos)
EXTRACTION_ENTITY=listings

# Configuración de API (Requerido si fuente es api)
# Para listings (simulación marketplace): https://dummyjson.com
# Para posts: https://jsonplaceholder.typicode.com
API_BASE_URL=https://dummyjson.com
# API Key si es requerida por la API
API_KEY=

# Configuración de Scraping (Requerido si fuente es scraping)
SCRAPING_BASE_URL=https://ejemplo.com

# Configuración de CSV (Opcional, si fuente es csv)
# Carpeta base donde se encuentran los archivos CSV organizados por entidad
CSV_DATA_FOLDER=datos